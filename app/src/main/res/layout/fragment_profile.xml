            <TextView
                android:id="@+id/user_profile_age"
                android:layout_width="0dp"
                android:layout_height="30dp"
                android:layout_marginStart="16dp"
                android:layout_marginEnd="24dp"
                android:text="age"
                android:textSize="24sp"
                app:layout_constraintBottom_toBottomOf="@+id/imageView_age"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/imageView_age"
                app:layout_constraintTop_toTopOf="@+id/imageView_age" />

            <TextView
                android:id="@+id/user_profile_height"
                android:layout_width="0dp"
                android:layout_height="30dp"
                android:layout_marginStart="16dp"
                android:layout_marginEnd="24dp"
                android:text="height"
                android:textSize="24sp"
                app:layout_constraintBottom_toBottomOf="@+id/imageView_height"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/imageView_height"
                app:layout_constraintTop_toTopOf="@+id/imageView_height" />

            <TextView
                android:id="@+id/user_profile_password"
                android:layout_width="0dp"
                android:layout_height="30dp"
                android:layout_marginStart="16dp"
                android:layout_marginEnd="24dp"
                android:text="password"
                android:textSize="24sp"
                app:layout_constraintBottom_toBottomOf="@+id/imageView7"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/imageView7"
                app:layout_constraintTop_toTopOf="@+id/imageView7" />



            <TextView
                android:id="@+id/user_profile_mail"
                android:layout_width="0dp"
                android:layout_height="33dp"
                android:layout_marginStart="16dp"
                android:layout_marginEnd="24dp"
                android:text="email"
                android:textSize="24sp"
                app:layout_constraintBottom_toBottomOf="@+id/imageview11"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/imageview11"
                app:layout_constraintTop_toTopOf="@+id/imageview11" />

            <TextView
                android:id="@+id/user_profile_name"
                android:layout_width="wrap_content"
                android:layout_height="33dp"
                android:layout_marginStart="24dp"
                android:layout_marginTop="80dp"
                android:layout_marginEnd="24dp"
                android:text="UserName"
                android:textColor="#040303"
                android:textSize="24sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <Button
                android:id="@+id/user_profile_edit_btn"
                android:layout_width="120dp"
                android:layout_height="45dp"
                android:layout_marginStart="200dp"
                android:layout_marginTop="60dp"
                android:layout_marginBottom="5dp"
                android:background="@drawable/roundedbutton"
                android:text="EDIT "
                android:textColor="#79098C"
                android:textSize="18sp"
                android:textStyle="bold"
                app:backgroundTint="#BEF3EFF4"
                app:layout_constraintBottom_toTopOf="@+id/user_profile_height"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.502"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/user_profile_password" />


            <ImageView
                android:id="@+id/imageview11"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:layout_marginStart="30dp"
                android:layout_marginTop="30dp"
                android:src="@drawable/email"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/user_profile_name" />



            <ImageView
                android:id="@+id/imageView7"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:layout_marginStart="30dp"
                android:layout_marginTop="30dp"
                android:src="@drawable/password"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageview11" />

            <ImageView
